{
    "name": "@cbnsndwch/ghl-sdk-webhooks",
    "description": "Webhook utilities for GHL SDK",
    "version": "0.3.6",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist",
        "prebuild": "pnpm clean",
        "build": "tsc",
        "watch": "tsc -w",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*\"",
        "lint:fix": "eslint \"src/**/*\" --fix"
    },
    "dependencies": {
        "@cbnsndwch/ghl-sdk-storage": "workspace:*"
    },
    "devDependencies": {
        "@types/express": "^5.0.5",
        "@types/node": "^20.0.0",
        "express": "^5.1.0",
        "rimraf": "^6.0.1",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "@cbnsndwch/ghl-sdk-storage": "^1.0.0"
    },
    "peerDependenciesMeta": {
        "express": {
            "optional": true
        }
    },
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./adapters/express": {
            "types": "./dist/adapters/express.d.ts",
            "default": "./dist/adapters/express.js"
        }
    },
    "engines": {
        "node": ">=22"
    },
    "publishConfig": {
        "access": "public"
    }
}
