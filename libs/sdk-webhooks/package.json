{
    "name": "@cbnsndwch/ghl-sdk-webhooks",
    "description": "Webhook utilities for GHL SDK",
    "version": "0.5.0",
    "type": "module",
    "license": "MIT",
    "author": {
        "name": "Sergio Leon",
        "url": "https://www.cbnsndwch.io"
    },
    "keywords": [
        "ghl",
        "highlevel",
        "cbnsndwch"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cbnsndwch/ghl-app-template.git",
        "directory": "libs/sdk-webhooks"
    },
    "scripts": {
        "clean": "rimraf dist",
        "prebuild": "pnpm clean",
        "build": "tsc",
        "watch": "tsc -w",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*\"",
        "lint:fix": "eslint \"src/**/*\" --fix"
    },
    "dependencies": {
        "@cbnsndwch/ghl-sdk-storage": "workspace:*"
    },
    "devDependencies": {
        "@types/express": "^5.0.5",
        "@types/node": "^20.0.0",
        "express": "^5.1.0",
        "rimraf": "^6.0.1",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "@cbnsndwch/ghl-sdk-storage": "^0.5.0"
    },
    "peerDependenciesMeta": {
        "express": {
            "optional": true
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./adapters/express": {
            "types": "./dist/adapters/express.d.ts",
            "default": "./dist/adapters/express.js"
        }
    },
    "engines": {
        "node": ">=22"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "packageManager": "pnpm@10.20.0"
}
