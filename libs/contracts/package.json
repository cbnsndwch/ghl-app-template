{
    "name": "@cbnsndwch/ghl-app-contracts",
    "description": "Data contracts for GHL Marketplace apps",
    "version": "0.4.0",
    "type": "module",
    "license": "MIT",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "author": {
        "name": "Sergio Leon",
        "url": "https://www.cbnsndwch.io"
    },
    "keywords": [
        "ghl",
        "highlevel",
        "cbnsndwch"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cbnsndwch/ghl-app-template.git",
        "directory": "libs/contracts"
    },
    "scripts": {
        "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
        "prebuild": "rimraf dist",
        "build": "tsc -b tsconfig.json",
        "watch": "tsc -b tsconfig.json -w",
        "prepublish": "pnpm build --force",
        "pub": "npm publish",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"src/**/*\"",
        "lint:fix": "eslint \"{src,test}/**/*\" --fix"
    },
    "devDependencies": {
        "@eslint/js": "^9.38.0",
        "@types/node": "^20.12.12",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "eslint": "^9.38.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^16.4.0",
        "mongoose": "^8.19.2",
        "prettier": "^3.6.2",
        "rimraf": "6.0.1",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.2"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "engines": {
        "node": ">=22"
    },
    "packageManager": "pnpm@10.20.0"
}
