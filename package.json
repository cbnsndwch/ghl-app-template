{
    "name": "@cbnsndwch/ghl-app",
    "description": "A base template for full stack HighLevel Marketplace apps",
    "version": "0.3.6",
    "license": "MIT",
    "private": true,
    "repository": "https://github.com/cbnsndwch/ghl-app",
    "author": {
        "name": "Sergio Leon",
        "url": "https://www.cbnsndwch.io"
    },
    "workspaces": {
        "packages": [
            "apps/*",
            "libs/*"
        ]
    },
    "scripts": {
        "postinstall": "turbo run build",
        "clean": "turbo run clean",
        "build": "turbo run build",
        "format": "turbo run format",
        "lint": "turbo run lint",
        "lint:fix": "turbo run lint:fix",
        "test": "turbo run test",
        "pub": "turbo run pub",
        "vmajor": "pnpm -r --workspace-concurrency=1 exec pnpm version major",
        "vminor": "pnpm -r --workspace-concurrency=1 exec pnpm version minor",
        "vpatch": "pnpm -r --workspace-concurrency=1 exec pnpm version patch",
        "clog": "conventional-changelog -p angular -i CHANGELOG.md -o changelog-temp.md"
    },
    "devDependencies": {
        "@eslint/js": "^9.38.0",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "change-case": "^5.4.4",
        "conventional-changelog-cli": "^4.1.0",
        "eslint": "^9.38.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "globals": "^16.4.0",
        "http-server": "^14.1.1",
        "npm-check-updates": "^19.1.2",
        "prettier": "^3.2.5",
        "turbo": "^2.5.8",
        "typescript-eslint": "^8.46.2"
    },
    "packageManager": "pnpm@10.20.0"
}
